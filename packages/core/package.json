{
  "name": "@requestly/core",
  "version": "1.0.0",
  "description": "Core rule processing logic for Requestly",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./rule-processor": {
      "require": "./dist/rule-processor/index.cjs.js",
      "import": "./dist/rule-processor/index.esm.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "files": [
    "dist"
  ]
}
