  "scripts": {
    "postinstall": "patch-package",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "test": "vitest --passWithNoTests",
    "lint": "eslint --fix --ext .js,.jsx .",
    "type-check": "tsc --noEmit",
    "build": "vite build",
    "build:emulator": "vite build --mode dev-emulator",
    "preview": "npm run build && vite preview --port 3000",
    "preview:emulator": "npm run build:emulator && vite preview --port 3000",
    "dev": "concurrently --kill-others --names \"@requestly/shared, @requestly/app\" \"cd ../shared && npm run watch\" \"vite\"",
    "dev:emulator": "concurrently --kill-others --names \"@requestly/shared, @requestly/app\" \"cd ../shared && npm run watch\" \"vite --mode dev-emulator\"",
    "start": "concurrently --kill-others --names \"@requestly/shared, @requestly/app\" \"cd ../shared && npm run watch\" \"vite\"",
    "start:emulator": "concurrently --kill-others --names \"@requestly/shared, @requestly/app\" \"cd ../shared && npm run watch\" \"vite --mode dev-emulator\"",
    "//": "****Deprecated****",
    "emulated-dev": "concurrently --kill-others --names \"@requestly/shared, @requestly/app\" \"cd ../shared && npm run watch\" \"vite --mode dev-emulator\"",
    "emulated-start": "vite --mode dev-emulator"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
